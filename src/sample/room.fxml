      </Button>
      <Button fx:id="fromRoomToKitchenButton" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" layoutX="55.0" layoutY="479.0" mnemonicParsing="false" onAction="#initialize" prefHeight="131.0" prefWidth="199.0">
