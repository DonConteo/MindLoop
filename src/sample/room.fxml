      <Button fx:id="fromRoomToBathButton" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" layoutX="1627.0" layoutY="479.0" mnemonicParsing="false" prefHeight="131.0" prefWidth="199.0">
